*{
	font-size: 14px;
}

.header{
	margin-bottom: 20px;
}

.breadcrumb{
	height:21px;
	padding:5px 0px;
	margin-bottom: 20px;
	/*background-color:red;*/
	/*border: 1px solid red;*/
}

.breadcrumb a:hover{
	color: #25bb9b;
}

.breadcrumb span{
	color: #777;
}

.container .form_group{
	height: 73px;
	border: 1px solid red;
	margin-bottom: 15px;
}

.container .form_group label{
	display:inline-block;
	margin-bottom: 5px;
	font-weight: 700;
}

.container .form_group #title{
	display: block;
	width: 100%;
	height: 24px;
	padding: 10px 16px;
	font-size: 18px;
	line-height: 1.3333333;
	border-radius: 6px;
}
