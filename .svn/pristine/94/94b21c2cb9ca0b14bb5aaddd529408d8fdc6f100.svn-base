package cc.siriuscloud.dtxz.bean.vo;

import java.util.List;

import cc.siriuscloud.dtxz.bean.Exam;
import cc.siriuscloud.dtxz.bean.User;

/**
 * @author sirius
 * 对于User的扩展，这里主要是存放一对多，一对一，等bean集合。
 */
public class UserVo extends User {
	

	private Long examSum;//考试数
	private Long rankSum;//排名
	private Long  accessSum;//被采纳数

	private Long  praiseSum;//获赞数
	private Long  correctSum;//正确率
	private Long  clazzSum;//班级数
	private Long  answerSum;//答题数
	
	
	private List<Exam> exams;
	
	public List<Exam> getExams() {
		return exams;
	}
	public void setExams(List<Exam> exams) {
		this.exams = exams;
	}
	public Long getExamSum() {
		return examSum;
	}
	public void setExamSum(Long examSum) {
		this.examSum = examSum;
	}
	public Long getRankSum() {
		return rankSum;
	}
	public void setRankSum(Long rankSum) {
		this.rankSum = rankSum;
	}
	public Long getAccessSum() {
		return accessSum;
	}
	public void setAccessSum(Long accessSum) {
		this.accessSum = accessSum;
	}
	public Long getPraiseSum() {
		return praiseSum;
	}
	public void setPraiseSum(Long praiseSum) {
		this.praiseSum = praiseSum;
	}
	public Long getCorrectSum() {
		return correctSum;
	}
	public void setCorrectSum(Long correctSum) {
		this.correctSum = correctSum;
	}
	public Long getClazzSum() {
		return clazzSum;
	}
	public void setClazzSum(Long clazzSum) {
		this.clazzSum = clazzSum;
	}
	public Long getAnswerSum() {
		return answerSum;
	}
	public void setAnswerSum(Long answerSum) {
		this.answerSum = answerSum;
	}
	
	@Override
	public String toString() {
		return "UserVo [考试数examSum=" + examSum + ", 总排名rankSum=" + rankSum + ", accessSum=" + accessSum + ", 点赞数praiseSum="
				+ praiseSum + ", correctSum=" + correctSum + ", clazzSum=" + clazzSum + ", answerSum=" + answerSum
				+ "]";
	}
	
	

}
