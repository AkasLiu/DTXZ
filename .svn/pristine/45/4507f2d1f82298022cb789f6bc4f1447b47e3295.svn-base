.wrap .container{
	/*height: 100px;
	background: red;*/
	margin-top: 20px;
}

.question_left{
	width: 700px;
	height:740px;
	padding: 0px 15px;
	float: left;
	background-color: #FFFFFF;
	margin-bottom: 20px;
	/*margin-top: 150px;*/
	/*background: red;*/
}

.question_right{
	width: 250px;
	height: 500px;
	float: right;
	/*margin-top: 150px;*/
	/*background: blue;*/
}

.question_left .question_list{
	height: 46px;
	padding: 10px 0px;
	border-bottom: 1px solid gray;
	/*background: gold;*/
}

.question_list .answer{
	/*display: inline-block;*/
	float: left;
	width: 46px;
	height: 46px;
    color: #fff;
    border-radius: 1px;
    font-size: 13px;
    text-align: center;
    line-height: 1.75;
}

.null{
	background-color: #AD3A37;
}

.exist{
	background-color: #009a61;
}

.question_list .summary{
	float: left;
	width: 	600px;
	height: 50px;
	margin-left: 25px;
	/*background: gold;*/
}

.summary .author{
	margin-bottom: 5px;
}

.summary .title span{
	font-size: 16px;
	color: #ff8a00;
}

.summary .title span i{
	display: inline-block;
	width:20px;
	height: 22px;
	position: relative;
	top:1px;
	/*font-size: 12px;*/
	
	background: url(../images/money.png) no-repeat -3px 4px;
	background-size: 23px;
}

.summary .title a{
	font-size: 16px;
}

.summary a:hover{
	color: #25bb9b;
	/*text-decoration: underline;*/
}

.question_right .alert{
	display: inline-block;
	width:231px;
	height: 65px;
	margin-bottom: 20px;
	padding: 15px;
	background-color: #fcf8e3;
	border-radius: 4px;
	border: 1px solid #fbeed5;
}

.question_right .alert p{
	color: #8a6d3b;
	font-size: 14px;
}

.question_right .alert a{
	display: block;
	width:205px;
	height: 20px;
	padding: 6px 12px;
	margin-top: 10px;
	border-color: #008151;
	color: #fff;
	text-align: center;
	border: 1px solid transparent;
	font-size: 14px;
	background-color:#009a61;
}

/*.foot{
	width: 100%;
	height: 100px;
	margin: 100px;
	background: red;
}*/

.last{
	margin-bottom: 20px;
}

.pagination-ul li{
	float:left;
	height: 32px;
	margin-right: 5px;
}

.pagination-ul li.first-page{
	width: 44px;
	margin-left: 100px;
}

.pagination-ul li.last-page{
	width: 44px;
}

.pagination-ul li.prev-page{
	width: 56px;
}

.pagination-ul li.next-page{
	width: 56px;
}

.pagination-ul li.page-number{
	width: 32px;
}

.pagination-ul li>a{
	display: block;
	height: 30px;
	text-align: center;
	line-height: 30px;
	border: 1px solid #e1e1e1;
	color: #999;
}