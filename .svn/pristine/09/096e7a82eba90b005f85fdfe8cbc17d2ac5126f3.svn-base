*{
	font-size: 14px;
}

.header{
	margin-bottom: 20px;
}

.breadcrumb{
	height:21px;
	padding:5px 0px;
	margin-bottom: 20px;
	/*background-color:red;*/
	/*border: 1px solid red;*/
}

.breadcrumb a:hover{
	color: #25bb9b;
}

.breadcrumb span{
	color: #777;
}

.container .ques_title{
	height: 73px;
	/*border: 1px solid red;*/
	margin-bottom: 15px;
}

.container .ques_title label{
	display:inline-block;
	margin-bottom: 5px;
	font-weight: 700;
}

.container .ques_title #title{
	display: block;
	width: 967px;
	height: 24px;
	padding: 10px 16px;
	font-size: 18px;
	line-height: 1.3333333;
	border-radius: 6px;
	border: 1px solid #ccc;
	color: #555;
	box-shadow: inset 0 1px 1px rgba(0,0,0,0.75);
}

.container .ques_title #title:focus {
    border-color: #66afe9;
    outline: 0;
    box-shadow: inset 0 1px 1px rgba(0,0,0,.075),0 0 8px rgba(102,175,233,.6);
}

.container .ques_content{
	height:303px;
	margin-bottom: 50px;
	border: 1px solid red;
}

.last .btn{
	display: inline-block;
	width: 90px;
	height: 44px;
	padding: 6px 12px;
	font-size: 14px;
	font-weight: 400;
	line-height: 1.42857143;
	text-align: center;
	vertical-align: middle;
	border-color: #008151;
	background-color: #009a61;
	color: #fff;
	/*border: 1px solid transparent;
	border-top-color: transparent;
	border-right-color: transparent;
	border-bottom-color: transparent;
	border-left-color: transparent;*/
	border-radius: 4px;
	float: right;	
}

.last .btn:hover{
	border-color: #00432a;
    background-color: #006741;
    color: #fff;
}