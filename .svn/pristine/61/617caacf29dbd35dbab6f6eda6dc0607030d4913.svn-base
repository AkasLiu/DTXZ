/************************** 通用设置 *****************/
* {
	margin: 0;
	padding: 0;
}

.left {
	float: left;
}

.right {
	float: right;
}

.clear{
	clear: both;
}

/* 通用辅助类 */

/* 页面路径 */
.crumbs-path {
	color: #666;
	font-size: 14px;
	margin: 13px 0;
}

.crumbs-path a {
	color: #666;
}

.crumbs-path a:hover {
	color: #25bb9b;
}

/* 带红点标题 */
.red-pot-title {
	font-size: 18px;
	height: 22px;
}


.red-pot-title::before {
	display: inline-block;
	content: '';
	width: 7px;
	height: 7px;
	margin: 5px;
	/*vertical-align: center;*/
	background: #FF6547;
}

.red-pot-title a {
	/*float: left;*/
	color: #25bb9b;
}

/******************* 容器类 ******************/
.adv-box {
	width: 100%;
	height: 200px;
	/*visibility: visible;*/
	text-align: center;
	background-color: #f7d840;
}

.big-container {
	margin: 0 auto;
	width: 960px;
}

.cur-exam-box {
	
}

.normal-box {
	width: 690px;
	padding: 15px;
	margin-bottom: 20px;
	font-size: 14px;
	color: #333;
	background-color: #fff;
	border-radius: 7px;
	box-shadow: 2px 2px 2px rgba(0, 0, 0, 0.2);
}

.normal-box .normal-content {
	padding: 10px 40px;
	overflow: hidden;
	text-overflow: ellipsis;
}

.normal-box table {
	margin: 0 auto;
	margin-top: 30px;
	margin-bottom: 30px;
	border: 0.1px solid rgba(0, 0, 0, 0.1);
}

.normal-box table th, td {
	padding: 14px 20px;
	letter-spacing: 1px;
	text-align: left;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.normal-box table th {
	color: #888;
	background-color: #EDEDED;
}

.normal-box table td {
	max-width: 300px;
	color: #666;
	border-bottom: 0.1px solid rgba(0, 0, 0, 0.1);
}

/* 元素类 */

/************* 广告 ****************/
.adv-box img {
	width: 960px;
	height: 200px;
}


/* 分页（申根） */
ul{
	list-style: none;
	overflow: hidden;
	text-align: center;
	margin-bottom: 20px;
}

.pagination-ul li{
	display: inline-block;
	height: 32px;
	margin-right: 5px;
}

.pagination-ul li:hover {
	background-color: #00bc9b;
}

.pagination-ul li:hover > a {
	color: white;
}

.pagination-ul li.first-page{
	width: 44px;
}

.pagination-ul li.last-page{
	width: 44px;
}

.pagination-ul li.prev-page{
	width: 56px;
}

.pagination-ul li.next-page{
	width: 56px;
}

.pagination-ul li.page-number{
	width: 32px;
}


.pagination-ul li>a{
	display: block;
	height: 30px;
	text-align: center;
	line-height: 30px;
	border: 1px solid #e1e1e1;
	color: #999;
}

.pagination-ul .active{
	background-color: #00bc9b;
	border-color: #00bc9b;
	/*box-sizing: border-box;*/
}

/* 页禁点击类 */
.pagination-ul .disabled a:hover {
	border-color:#e3e3e3;
 	background:#e3e3e3;
 	color:#FFF;
 	cursor:not-allowed;
}

.active>a>p{
	color: #FFF;
}